    "scripts": {
        "build": "rimraf lib && tsc",
        "watch": "tsc -w",
        "format": "prettier --config ./.prettierrc.js --write ./src/**/*.{ts,js}",
        "lint": "tsc --noEmit && eslint src/**/*.{ts,tsx} --quiet --fix"
